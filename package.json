{
  "name": "aural",
  "version": "0.6.1",
  "description": "The zero-dependency audio manager for your Javascript projects.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rollup -i ./src/index.js -o ./dist/index.js -f cjs -c rollup.config.js && gzip-size ./dist/index.js",
    "watch": "rollup -w -i ./src/index.js -o ./dist/index.js -f cjs -c rollup.config.js && gzip-size ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mjanssen/aural.git"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100
  },
  "author": {
    "name": "Marnix Janssen",
    "email": "dev.marnix@gmail.com",
    "url": "https://github.com/mjanssen"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "gzip-size-cli": "^2.1.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {}
}
